<span class="cp">#define TMAX 40
</span>
<span class="k">typedef</span> <span class="k">struct</span> <span class="p">{</span>
	<span class="kt">char</span> <span class="n">character</span><span class="p">[</span><span class="n">TMAX</span><span class="p">];</span>
	<span class="kt">char</span> <span class="n">name</span><span class="p">[</span><span class="n">TMAX</span><span class="p">];</span>
<span class="p">}</span><span class="n">Item</span><span class="p">;</span>

<span class="kt">void</span> <span class="nf">print_item_array</span><span class="p">(</span><span class="n">Item</span> <span class="n">items</span><span class="p">[],</span> <span class="kt">int</span> <span class="n">size</span><span class="p">)</span>
<span class="p">{</span>
	<span class="k">for</span> <span class="p">(</span><span class="kt">int</span> <span class="n">i</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span> <span class="n">i</span> <span class="o">&lt;</span> <span class="n">size</span><span class="p">;</span> <span class="o">++</span><span class="n">i</span><span class="p">)</span>
		<span class="n">printf</span><span class="p">(</span><span class="s">"%s (%s)</span><span class="se">\n</span><span class="s">"</span><span class="p">,</span> <span class="n">items</span><span class="p">[</span><span class="n">i</span><span class="p">].</span><span class="n">character</span><span class="p">,</span> <span class="n">items</span><span class="p">[</span><span class="n">i</span><span class="p">].</span><span class="n">name</span><span class="p">);</span>
	<span class="n">printf</span><span class="p">(</span><span class="s">"</span><span class="se">\n</span><span class="s">"</span><span class="p">);</span>
<span class="p">}</span>

<span class="c1">// sorting í•  ë•Œ í•„ìš”í•œ ë¹„êµ í•¨ìˆ˜</span>
<span class="kt">int</span> <span class="nf">compare_items</span><span class="p">(</span><span class="k">const</span> <span class="kt">void</span><span class="o">*</span> <span class="n">first</span><span class="p">,</span> <span class="k">const</span> <span class="kt">void</span><span class="o">*</span> <span class="n">second</span><span class="p">)</span> 
<span class="p">{</span>
	<span class="n">Item</span><span class="o">*</span> <span class="n">first_item</span> <span class="o">=</span> <span class="p">(</span><span class="n">Item</span><span class="o">*</span><span class="p">)</span><span class="n">first</span><span class="p">;</span>
	<span class="n">Item</span><span class="o">*</span> <span class="n">second_item</span> <span class="o">=</span> <span class="p">(</span><span class="n">Item</span><span class="o">*</span><span class="p">)</span><span class="n">second</span><span class="p">;</span>

	<span class="k">return</span> <span class="n">strcmp</span><span class="p">(</span><span class="n">first_item</span><span class="o">-&gt;</span><span class="n">character</span><span class="p">,</span> <span class="n">second_item</span><span class="o">-&gt;</span><span class="n">character</span><span class="p">);</span>
<span class="p">}</span>

<span class="c1">// ìˆœì°¨ íƒìƒ‰</span>
<span class="k">const</span> <span class="n">Item</span><span class="o">*</span> <span class="nf">sequential_search</span><span class="p">(</span><span class="k">const</span> <span class="n">Item</span> <span class="n">items</span><span class="p">[],</span> <span class="k">const</span> <span class="kt">int</span> <span class="n">size</span><span class="p">,</span> <span class="k">const</span> <span class="kt">char</span> <span class="n">key</span><span class="p">[</span><span class="n">TMAX</span><span class="p">])</span>
<span class="p">{</span>
	<span class="k">for</span> <span class="p">(</span><span class="kt">int</span> <span class="n">i</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span> <span class="n">i</span> <span class="o">&lt;</span> <span class="n">size</span><span class="p">;</span> <span class="o">++</span><span class="n">i</span><span class="p">)</span>
		<span class="k">if</span> <span class="p">(</span><span class="n">strcmp</span><span class="p">(</span><span class="n">items</span><span class="p">[</span><span class="n">i</span><span class="p">].</span><span class="n">character</span><span class="p">,</span> <span class="n">key</span><span class="p">)</span> <span class="o">==</span> <span class="mi">0</span><span class="p">)</span>
			<span class="k">return</span> <span class="o">&amp;</span><span class="n">items</span><span class="p">[</span><span class="n">i</span><span class="p">];</span>

	<span class="k">return</span> <span class="nb">NULL</span><span class="p">;</span>
<span class="p">}</span>

<span class="c1">// ì´ì§„ íƒìƒ‰</span>
<span class="k">const</span> <span class="n">Item</span><span class="o">*</span> <span class="nf">binary_search</span><span class="p">(</span><span class="k">const</span> <span class="n">Item</span> <span class="n">items</span><span class="p">[],</span> <span class="k">const</span> <span class="kt">int</span> <span class="n">size</span><span class="p">,</span> <span class="k">const</span> <span class="kt">char</span> <span class="n">key</span><span class="p">[</span><span class="n">TMAX</span><span class="p">])</span>
