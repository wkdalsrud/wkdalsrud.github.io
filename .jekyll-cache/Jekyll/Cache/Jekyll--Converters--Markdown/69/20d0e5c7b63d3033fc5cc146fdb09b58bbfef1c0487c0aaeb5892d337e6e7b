		<span class="k">else</span>
			<span class="n">printf</span><span class="p">(</span><span class="s">"%s is %s</span><span class="se">\n\n</span><span class="s">"</span><span class="p">,</span> <span class="n">found</span><span class="o">-&gt;</span><span class="n">character</span><span class="p">,</span> <span class="n">found</span><span class="o">-&gt;</span><span class="n">name</span><span class="p">);</span>
	<span class="p">}</span>
