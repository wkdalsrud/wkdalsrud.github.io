<span class="kt">void</span> <span class="nf">BFS</span><span class="p">(</span><span class="n">Pos</span> <span class="n">start</span><span class="p">)</span> <span class="p">{</span>

    <span class="n">queue</span><span class="o">&lt;</span><span class="n">Pos</span><span class="o">&gt;</span> <span class="n">q</span><span class="p">;</span>
