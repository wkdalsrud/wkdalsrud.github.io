<span class="p">{</span>
    <span class="n">printf</span><span class="p">(</span><span class="s">"\u250C\u252C\u2510</span><span class="se">\n</span><span class="s">"</span><span class="p">);</span>
    <span class="n">printf</span><span class="p">(</span><span class="s">"\u251C\u253C\u2524</span><span class="se">\n</span><span class="s">"</span><span class="p">);</span>
    <span class="n">printf</span><span class="p">(</span><span class="s">"\u2514\u2534\u2518</span><span class="se">\n</span><span class="s">"</span><span class="p">);</span>
<span class="p">}</span>
