	<span class="k">else</span> <span class="k">if</span> <span class="p">(</span><span class="n">comp</span> <span class="o">&lt;</span> <span class="mi">0</span><span class="p">)</span>
		<span class="k">return</span> <span class="n">Search</span><span class="p">(</span><span class="n">root</span><span class="o">-&gt;</span><span class="n">left</span><span class="p">,</span> <span class="n">key</span><span class="p">);</span>  <span class="c1">// recursion</span>
	<span class="k">else</span> <span class="k">if</span> <span class="p">(</span><span class="n">comp</span> <span class="o">&gt;</span> <span class="mi">0</span><span class="p">)</span>
		<span class="k">return</span> <span class="n">Search</span><span class="p">(</span><span class="n">root</span><span class="o">-&gt;</span><span class="n">right</span><span class="p">,</span> <span class="n">key</span><span class="p">);</span> <span class="c1">// recursion</span>
<span class="p">}</span>


