<span class="n">Item</span><span class="o">*</span> <span class="nf">TreeSearch</span><span class="p">(</span><span class="n">Tree</span><span class="o">*</span> <span class="n">tree</span><span class="p">,</span> <span class="k">const</span> <span class="n">Item</span> <span class="n">key</span><span class="p">);</span>
