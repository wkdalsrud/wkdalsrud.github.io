	<span class="k">return</span> <span class="n">strcmp</span><span class="p">(</span><span class="n">first</span><span class="p">.</span><span class="n">character</span><span class="p">,</span> <span class="n">second</span><span class="p">.</span><span class="n">character</span><span class="p">);</span>
