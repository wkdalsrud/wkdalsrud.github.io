<span class="kt">bool</span> <span class="nf">AddItem</span><span class="p">(</span><span class="k">const</span> <span class="n">Item</span><span class="o">*</span> <span class="n">pi</span><span class="p">,</span> <span class="n">Tree</span><span class="o">*</span> <span class="n">ptree</span><span class="p">)</span>
