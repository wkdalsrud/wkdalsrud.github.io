        <span class="n">kind</span><span class="p">.</span><span class="n">insert</span><span class="p">(</span><span class="n">gems</span><span class="p">[</span><span class="n">i</span><span class="p">]);</span> <span class="c1">// setμ€ μ¤‘λ³µμ„ μ κ±°ν•λ―€λ΅ μµμΆ…μ μΌλ΅ kind.size() λ” λ³΄μ„μ μΆ…λ¥ κ°μκ°€ λλ‹¤.</span>
    
    <span class="kt">int</span> <span class="n">minLen</span> <span class="o">=</span> <span class="mi">100000</span><span class="p">;</span> <span class="c1">// κµ¬ν•λ” κµ¬κ°„</span>
