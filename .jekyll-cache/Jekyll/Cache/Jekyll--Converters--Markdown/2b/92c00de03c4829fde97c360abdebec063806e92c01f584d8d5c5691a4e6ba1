    <span class="k">if</span> <span class="p">(</span><span class="n">block</span> <span class="o">==</span> <span class="mi">6</span><span class="p">)</span> <span class="p">{</span>
        <span class="n">Pos</span> <span class="n">black1</span><span class="p">{</span> <span class="n">pos1</span><span class="p">.</span><span class="n">i</span><span class="p">,</span> <span class="n">pos1</span><span class="p">.</span><span class="n">j</span> <span class="o">+</span> <span class="mi">1</span> <span class="p">};</span> <span class="n">Pos</span> <span class="n">black2</span><span class="p">{</span> <span class="n">pos1</span><span class="p">.</span><span class="n">i</span> <span class="o">+</span> <span class="mi">1</span><span class="p">,</span> <span class="n">pos1</span><span class="p">.</span><span class="n">j</span> <span class="o">+</span> <span class="mi">1</span> <span class="p">};</span>
        <span class="k">if</span> <span class="p">(</span><span class="n">Board</span><span class="p">[</span><span class="n">black1</span><span class="p">.</span><span class="n">i</span><span class="p">][</span><span class="n">black1</span><span class="p">.</span><span class="n">j</span><span class="p">]</span> <span class="o">==</span> <span class="mi">0</span> <span class="o">&amp;&amp;</span> <span class="n">Board</span><span class="p">[</span><span class="n">black2</span><span class="p">.</span><span class="n">i</span><span class="p">][</span><span class="n">black2</span><span class="p">.</span><span class="n">j</span><span class="p">]</span> <span class="o">==</span> <span class="mi">0</span><span class="p">)</span> <span class="p">{</span>
            <span class="n">blackblocks</span><span class="p">.</span><span class="n">push_back</span><span class="p">(</span><span class="n">black1</span><span class="p">);</span>
            <span class="n">blackblocks</span><span class="p">.</span><span class="n">push_back</span><span class="p">(</span><span class="n">black2</span><span class="p">);</span>
        <span class="p">}</span>
    <span class="p">}</span>
    <span class="k">if</span> <span class="p">(</span><span class="n">block</span> <span class="o">==</span> <span class="mi">7</span><span class="p">)</span> <span class="p">{</span>
        <span class="n">Pos</span> <span class="n">black1</span><span class="p">{</span> <span class="n">pos1</span><span class="p">.</span><span class="n">i</span><span class="p">,</span> <span class="n">pos1</span><span class="p">.</span><span class="n">j</span> <span class="o">-</span> <span class="mi">2</span> <span class="p">};</span> <span class="n">Pos</span> <span class="n">black2</span><span class="p">{</span> <span class="n">pos1</span><span class="p">.</span><span class="n">i</span><span class="p">,</span> <span class="n">pos1</span><span class="p">.</span><span class="n">j</span> <span class="o">-</span> <span class="mi">1</span> <span class="p">};</span>
        <span class="k">if</span> <span class="p">(</span><span class="n">Board</span><span class="p">[</span><span class="n">black1</span><span class="p">.</span><span class="n">i</span><span class="p">][</span><span class="n">black1</span><span class="p">.</span><span class="n">j</span><span class="p">]</span> <span class="o">==</span> <span class="mi">0</span> <span class="o">&amp;&amp;</span> <span class="n">Board</span><span class="p">[</span><span class="n">black2</span><span class="p">.</span><span class="n">i</span><span class="p">][</span><span class="n">black2</span><span class="p">.</span><span class="n">j</span><span class="p">]</span> <span class="o">==</span> <span class="mi">0</span><span class="p">)</span> <span class="p">{</span>
            <span class="n">blackblocks</span><span class="p">.</span><span class="n">push_back</span><span class="p">(</span><span class="n">black1</span><span class="p">);</span>
            <span class="n">blackblocks</span><span class="p">.</span><span class="n">push_back</span><span class="p">(</span><span class="n">black2</span><span class="p">);</span>
        <span class="p">}</span>
    <span class="p">}</span>
    <span class="k">if</span> <span class="p">(</span><span class="n">block</span> <span class="o">==</span> <span class="mi">9</span><span class="p">)</span> <span class="p">{</span>
        <span class="n">Pos</span> <span class="n">black1</span><span class="p">{</span> <span class="n">pos1</span><span class="p">.</span><span class="n">i</span><span class="p">,</span> <span class="n">pos1</span><span class="p">.</span><span class="n">j</span> <span class="o">-</span> <span class="mi">1</span> <span class="p">};</span> <span class="n">Pos</span> <span class="n">black2</span><span class="p">{</span> <span class="n">pos1</span><span class="p">.</span><span class="n">i</span><span class="p">,</span> <span class="n">pos1</span><span class="p">.</span><span class="n">j</span> <span class="o">+</span> <span class="mi">1</span> <span class="p">};</span>
        <span class="k">if</span> <span class="p">(</span><span class="n">Board</span><span class="p">[</span><span class="n">black1</span><span class="p">.</span><span class="n">i</span><span class="p">][</span><span class="n">black1</span><span class="p">.</span><span class="n">j</span><span class="p">]</span> <span class="o">==</span> <span class="mi">0</span> <span class="o">&amp;&amp;</span> <span class="n">Board</span><span class="p">[</span><span class="n">black2</span><span class="p">.</span><span class="n">i</span><span class="p">][</span><span class="n">black2</span><span class="p">.</span><span class="n">j</span><span class="p">]</span> <span class="o">==</span> <span class="mi">0</span><span class="p">)</span> <span class="p">{</span>
