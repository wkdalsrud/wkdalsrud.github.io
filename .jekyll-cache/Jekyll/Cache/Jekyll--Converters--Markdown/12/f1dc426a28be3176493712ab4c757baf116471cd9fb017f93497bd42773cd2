                <span class="c1">// 1-1. ìœ„ìª½ì„ ì¶•ìœ¼ë¡œ ì™¼ìª½ìœ¼ë¡œ íšŒì „</span>
                <span class="k">if</span> <span class="p">(</span><span class="o">!</span><span class="n">checked</span><span class="p">[</span><span class="n">left_up</span><span class="p">.</span><span class="n">y</span><span class="p">][</span><span class="n">left_up</span><span class="p">.</span><span class="n">x</span><span class="p">][</span><span class="n">LEFT</span><span class="p">]</span> <span class="o">||</span> <span class="o">!</span><span class="n">checked</span><span class="p">[</span><span class="n">now</span><span class="p">.</span><span class="n">part1</span><span class="p">.</span><span class="n">y</span><span class="p">][</span><span class="n">now</span><span class="p">.</span><span class="n">part1</span><span class="p">.</span><span class="n">x</span><span class="p">][</span><span class="n">RIGHT</span><span class="p">])</span> <span class="p">{</span>
