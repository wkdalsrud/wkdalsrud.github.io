                <span class="k">if</span> <span class="p">(</span><span class="o">!</span><span class="n">checked</span><span class="p">[</span><span class="n">right_down</span><span class="p">.</span><span class="n">y</span><span class="p">][</span><span class="n">right_down</span><span class="p">.</span><span class="n">x</span><span class="p">][</span><span class="n">RIGHT</span><span class="p">]</span> <span class="o">||</span> <span class="o">!</span><span class="n">checked</span><span class="p">[</span><span class="n">now</span><span class="p">.</span><span class="n">part2</span><span class="p">.</span><span class="n">y</span><span class="p">][</span><span class="n">now</span><span class="p">.</span><span class="n">part2</span><span class="p">.</span><span class="n">x</span><span class="p">][</span><span class="n">LEFT</span><span class="p">])</span> <span class="p">{</span>
