                <span class="n">Robot</span> <span class="n">next2</span><span class="p">{</span> <span class="p">{</span> <span class="n">now</span><span class="p">.</span><span class="n">part2</span><span class="p">.</span><span class="n">y</span><span class="p">,</span> <span class="n">now</span><span class="p">.</span><span class="n">part2</span><span class="p">.</span><span class="n">x</span><span class="p">,</span> <span class="n">UP</span> <span class="p">}</span> <span class="p">,</span> <span class="n">down_right</span><span class="p">,</span> <span class="n">now</span><span class="p">.</span><span class="n">time</span> <span class="o">+</span> <span class="mi">1</span> <span class="p">};</span>
                <span class="k">if</span> <span class="p">(</span><span class="n">checked</span><span class="p">.</span><span class="n">find</span><span class="p">(</span><span class="n">next2</span><span class="p">)</span> <span class="o">==</span> <span class="n">checked</span><span class="p">.</span><span class="n">end</span><span class="p">())</span> <span class="p">{</span>
