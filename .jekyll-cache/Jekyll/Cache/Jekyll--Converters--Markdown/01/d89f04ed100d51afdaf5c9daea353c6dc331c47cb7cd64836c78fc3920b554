<span class="kt">void</span> <span class="nf">readData</span><span class="p">(</span><span class="kt">char</span> <span class="o">*</span> <span class="n">name</span><span class="p">)</span>  <span class="c1">// Tab ìœ¼ë¡œ êµ¬ë¶„ë˜ì–´ ìˆëŠ” íŒŒì¼ì˜ ë°ì´í„°ë“¤ì„ ì½ê¸°</span>
<span class="p">{</span>
    <span class="kt">char</span> <span class="n">buffer</span><span class="p">[</span><span class="n">BUFFER_SIZE</span><span class="p">];</span>
